{
  "name": "personal-blog",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node --env-file=.env dist/index",
    "dev:server": "tsx watch --env-file=.env src/index",
    "dev:css": "npx tailwindcss -i src/views/assets/tailwind.css -o src/public/styles/main.css --watch",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:css\"",
    "build:db": "prisma generate && prisma db push",
    "build:server": "tsc && cp -r ./src/views ./dist && cp -r ./src/public ./dist",
    "build": "npm run build:db && npm run build:server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/connect-flash": "^0.0.40",
    "@types/express": "^4.17.21",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/express-session": "^1.18.0",
    "@types/method-override": "^0.0.35",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
